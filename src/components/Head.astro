---
import {Font} from "astro:assets"
import { ClientRouter } from "astro:transitions";
interface Props  {
    title?: string;
}
const {title = "Yasaman Moussavi's Official Website"} = Astro.props
---

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <meta name="generator" content={Astro.generator} />
    <Font cssVariable="--font-black-han" preload />
    <Font cssVariable="--font-karla" preload />
    <title>{title}</title>
    <ClientRouter />
    <slot />
</head>